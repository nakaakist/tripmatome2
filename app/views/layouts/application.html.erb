<!DOCTYPE html>
<html>
<head>
  <title>Tripmatome2</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<body>
<div>
  <% if current_user %>
    Signed in as <strong><%= current_user.nickname %></strong>!
    <%= link_to "Sign out", logout_path, id: "sign_out" %>
  <% else %>
    <%= link_to "Sign in with Twitter", "/auth/twitter", id: "sign_in" %>
  <% end %>
</div>
<%= yield %>

</body>
</html>
